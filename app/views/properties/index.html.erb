<span class="search">
  <%= form_tag(search_path, :method => "get") do %>
    <%= text_field_tag :search, params[:search]%>
    <%= button_tag "Search",:name => nil %>
  <% end %>
</span>
<h1>Properties</h1>

<div id="properties">
  <% @properties.each do |property| %>
    <%= render property %>
    <p>
      <%= link_to "Show this property", property %>
    </p>
  <% end %>
</div>
<div class="pagination"><div class="page" style="text-align:center;"><%= will_paginate @properties, previous_label: "&laquo;", next_label: "&raquo;"  %></div></div>
